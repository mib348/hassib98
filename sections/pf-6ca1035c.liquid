    </script><script>(function () {
  'use strict';

  // Configuration
  const PHP_ENDPOINT = 'https://app.sushi.catering';

  let isSubmitting = false;
  let hasInterceptedForm = false;

  function initFormIntercept() {
    // Clear success message and URL parameters on page load
    handleSuccessRedirect();

    // Wait for any form to be fully loaded and set up interception for all contact forms
    const checkForForms = () => {
      // Look for multiple form types
      const forms = document.querySelectorAll([
        'form[action*="contact"]',
        'form#captcha_form',
        'form#contact_form',
        'form.contact-form'
      ].join(', '));

      if (forms.length > 0) {

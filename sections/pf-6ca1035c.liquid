        forms.forEach((form, index) => {
          console.log(`Intercepting form ${index + 1}:`, form.action, form.id, form.className);
          interceptFormSubmission(form);
        });
      } else {
        setTimeout(checkForForms, 100);
      }
    };
    checkForForms();
  }

  function handleSuccessRedirect() {
    // Check if we're on a success redirect page
    if (window.location.search.includes('contact_posted=true')) {
      // Clear the URL parameter and remove any success messages
      const url = new URL(window.location);
      url.searchParams.delete('contact_posted');
      window.history.replaceState({}, document.title, url.toString());

      // Remove any existing success messages
      setTimeout(() => {
        const successMessages = document.querySelectorAll('p.sc-fHsiGf, .sc-jztuXx.bMfeNT');
        successMessages.forEach(msg => {
          if (msg.textContent.includes('Danke für deine Anfrage')) {
            msg.remove();
          }
        });
      }, 100);
    }
  }

  function interceptFormSubmission(form) {
    // Prevent duplicate interception
    if (form.dataset.intercepted === 'true') {
      return;
    }
    form.dataset.intercepted = 'true';

    console.log('Setting up interception for form:', form.action);

    // Primary interception: form submit event with highest priority
    form.addEventListener('submit', function (e) {
      console.log('Form submit intercepted:', form.action);
      e.preventDefault();
      e.stopImmediatePropagation();

      if (isSubmitting) {
        console.log('Already submitting, ignoring...');
        return false;
      }

      handleFormSubmission(form);
      return false;
    }, true);

    // Secondary interception: button click events
    const submitButtons = form.querySelectorAll('input[type="submit"], button[type="submit"], button:not([type])');
    submitButtons.forEach(button => {
      button.addEventListener('click', function (e) {
        console.log('Submit button clicked for form:', form.action);
        e.preventDefault();
        e.stopImmediatePropagation();

        if (isSubmitting) {
          return false;
        }

        handleFormSubmission(form);
        return false;
      }, true);
    });

    // Override form.submit() method as backup
    const originalSubmit = form.submit;
    form.submit = function () {
      console.log('form.submit() called for:', form.action);
      if (!isSubmitting) {
        handleFormSubmission(form);
      }
    };
  }

  function handleFormSubmission(form) {
    isSubmitting = true;

    // Clear any existing messages
    clearMessages();

    // Show loading state
    const submitButtons = form.querySelectorAll('input[type="submit"], button[type="submit"], button:not([type])');
    const originalButtonTexts = [];

    submitButtons.forEach((button, index) => {
      originalButtonTexts[index] = button.value || button.textContent;
      if (button.tagName === 'INPUT') {
        button.value = 'Senden...';
      } else {
        button.textContent = 'Senden...';
      }
      button.disabled = true;
    });

    // Extract contact data from form (works for both PageFly and Shopify forms)
    const formData = extractContactData(form);
    const returnPath = getReturnPath(form);

    console.log('Extracted form data:', formData);
    console.log('Return path:', returnPath);

    // Basic validation
    const validation = validateForm(formData);
    if (!validation.isValid) {
      showMessage(validation.message, 'error');
      resetButtons(submitButtons, originalButtonTexts);
      isSubmitting = false;
      return;
    }

    // Send to Laravel API
    sendToLaravelApi(formData)
      .then(response => {
        if (response.success) {
          showMessage('Danke für deine Anfrage, wir melden uns bei dir in Kürze!', 'success');

          // Redirect using the same path as original form after showing message
          setTimeout(() => {
            if (returnPath) {
              window.location.href = returnPath + '?contact_posted=true#contact_form';
            } else {
              // Fallback to current page with success parameter
              const currentUrl = new URL(window.location);
              currentUrl.searchParams.set('contact_posted', 'true');
              currentUrl.hash = 'contact_form';
              window.location.href = currentUrl.toString();
            }
          }, 2000);
        } else {
          showMessage(response.message || 'Leider konnte die Eingabe nicht gesendet werden! Bitte versuchen Sie es noch einmal!', 'error');
          resetButtons(submitButtons, originalButtonTexts);
          isSubmitting = false;
        }
      })
      .catch(error => {
        console.error('Form submission error:', error);
        showMessage('Leider konnte die Eingabe nicht gesendet werden! Bitte versuchen Sie es noch einmal!', 'error');
        resetButtons(submitButtons, originalButtonTexts);
        isSubmitting = false;
      });
  }

  function extractContactData(form) {
    const data = {};

    // Try PageFly format first (contact[fieldName])
    const contactInputs = form.querySelectorAll('input[name^="contact["]');
    if (contactInputs.length > 0) {
      contactInputs.forEach(input => {
        const fieldName = input.name.replace('contact[', '').replace(']', '');
        data[fieldName] = input.value;
      });
    } else {
      // Fallback to regular Shopify contact form format
      const allInputs = form.querySelectorAll('input[type="text"], input[type="email"], textarea');
      allInputs.forEach(input => {
        if (input.name && input.name !== 'h-captcha-response') {
          // Map common field names
          const fieldName = mapFieldName(input.name, input.placeholder, input.id);
          if (fieldName) {
            data[fieldName] = input.value;
          }
        }
      });
    }

    return data;
  }

  function mapFieldName(name, placeholder, id) {
    // Map form field names to our expected format
    const mappings = {
      'name': 'Dein Name',
      'phone': 'Deine Telefonnummer',
      'company': 'Dein Unternehmen',
      'employees': 'Mitarbeiteranzahl',
      'email': 'email'
    };

    // Check by name first
    if (mappings[name]) return mappings[name];

    // Check by placeholder
    if (placeholder) {
      if (placeholder.includes('Name')) return 'Dein Name';
      if (placeholder.includes('Telefon')) return 'Deine Telefonnummer';
      if (placeholder.includes('Unternehmen')) return 'Dein Unternehmen';
      if (placeholder.includes('Mitarbeiter')) return 'Mitarbeiteranzahl';
      if (placeholder.includes('Email') || placeholder.includes('E-Mail')) return 'email';
    }

    return name; // Fallback to original name
  }

  function getReturnPath(form) {
    // Extract return_path from hidden input (PageFly forms)
    const returnPathInput = form.querySelector('input[name="return_path"]');
    if (returnPathInput) {
      return returnPathInput.value;
    }

    // For regular Shopify forms, use current path
    return window.location.pathname;
  }

  function validateForm(data) {
    // Check required fields - be flexible with field names
    const requiredFields = [
      { keys: ['Dein Unternehmen', 'company'], label: 'Unternehmen' },
      { keys: ['Mitarbeiteranzahl', 'employees'], label: 'Mitarbeiteranzahl' },
      { keys: ['email'], label: 'Email' }
    ];

    for (let field of requiredFields) {
      let hasValue = false;
      for (let key of field.keys) {
        if (data[key] && data[key].trim() !== '') {
          hasValue = true;
          break;
        }
      }
      if (!hasValue) {
        return {
          isValid: false,
          message: 'Field can not be blank'
        };
      }
    }

    // Email validation
    const email = data.email || data['email'];
    if (email && !isValidEmail(email)) {
      return {
        isValid: false,
        message: 'Leider konnte die Eingabe nicht gesendet werden! Bitte versuchen Sie es noch einmal!'
      };
    }

    return { isValid: true };
  }

  function isValidEmail(email) {
    const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    return emailRegex.test(email);
  }

  function sendToLaravelApi(data) {
    return fetch(PHP_ENDPOINT + '/api/contact-handler', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
        'X-Requested-With': 'XMLHttpRequest',
        'Accept': 'application/json'
      },
      body: JSON.stringify(data)
    })
      .then(response => {
        if (!response.ok) {
          throw new Error(`HTTP error! status: ${response.status}`);
        }
        return response.json();
      });
  }

  function showMessage(message, type) {
    // Create message element
    const messageDiv = document.createElement('div');
    messageDiv.className = `form-message ${type}`;
    messageDiv.innerHTML = `<p class="sc-jztuXx bMfeNT">${message}</p>`;
    messageDiv.style.cssText = `
            margin: 10px 0;
            padding: 15px;
            border-radius: 4px;
            ${type === 'success' ? 'background-color: #d4edda; color: #155724; border: 1px solid #c3e6cb;' : 'background-color: #f8d7da; color: #721c24; border: 1px solid #f5c6cb;'}
            position: relative;
            z-index: 1000;
        `;

    // Find best insertion point
    const forms = document.querySelectorAll('form[action*="contact"], form#contact_form');
    if (forms.length > 0) {
      const form = forms[forms.length - 1]; // Use the last form found
      form.insertBefore(messageDiv, form.firstChild);
      messageDiv.scrollIntoView({ behavior: 'smooth', block: 'center' });
    }
  }

  function clearMessages() {
    const existingMessages = document.querySelectorAll('.form-message');
    existingMessages.forEach(msg => msg.remove());

    // Also remove any Shopify success messages
    const shopifyMessages = document.querySelectorAll('p.sc-fHsiGf, .sc-jztuXx.bMfeNT');
    shopifyMessages.forEach(msg => {
      if (msg.textContent.includes('Danke für deine Anfrage')) {
        msg.remove();
      }
    });
  }

  function resetButtons(buttons, originalTexts) {
    buttons.forEach((button, index) => {
      if (button.tagName === 'INPUT') {
        button.value = originalTexts[index];
      } else {
        button.textContent = originalTexts[index];
      }
      button.disabled = false;
    });
  }

  // Initialize and re-initialize when DOM changes (for dynamic forms)
